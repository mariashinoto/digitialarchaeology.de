  url = {https://www.jstor.org/stable/124087},
  urldate = {2023-10-04},
  abstract = {The high-speed computer has made feasible procedures for studying complex data that were impracticable a dozen years ago. To find which likely procedures are of most use to archaeologists, experimental analyses have been carried out. Some preliminary results on two well-known sets of material are presented: first, Upper Palaeolithic assemblages of stone tools published quantitatively by de Sonneville-Bordes and others. The sites chosen, broadly Aurignacian and Gravettian, include La Chevre, Les Vachons, La Ferrassie, Laugerie Haute, Vogelherd and Dolni Vestonice. Results from two structure-seeking methods are described: Average-Link Cluster Analysis provides a detailed taxonomy; Principal Components Analysis is able to suggest a more general pattern of relationships. Second, some prehistoric copper and bronze objects, spectrographically examined (by the 'Stuttgart School') for their metal content. An Average-Link Cluster Analysis, while not entirely satisfactory, defines several groups with a demonstrable significance.},
  timestamp = {2023-10-04T23:50:26Z},
  file = {Hodson - 1969 - Searching for Structure within Multivariate Archaeological Data.pdf:/Users/mariashinoto/Zotero-Daten/storage/BHSHEXER/Hodson - 1969 - Searching for Structure within Multivariate Archaeological Data.pdf:application/pdf}
}

@article{HodsonEtAl1966,
